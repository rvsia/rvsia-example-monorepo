{
    "branches": [
        "master",
        {"name": "beta", "channel": "beta", "prelease": "beta"}
    ],
    "monorepo": "./packages",
    "extends": "semantic-release-monorepo",
    "plugins": [
        [
            "@khala/commit-analyzer-wildcard",
            {
              "patterns": {
                "major": "^(majormajor|breakingbreaking)\\(",
                "minor": "^feat\\(",
                "patch": "^(fix|patch)\\("
              },
              "defaultRelease": "no-release"
            }
          ]
    ]
}